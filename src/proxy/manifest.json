{
  "version": "1.0.0",
  "manifest_version": 2,
  "name": "Chrome Proxy",

  "permissions": [
    "plugins",
    "tabs",
    "unlimitedStorage",
    "storage",
    "<all_urls>",
    "webRequest",
    "webRequestBlocking"
  ],

  "background": {
    "scripts": ["background.js"]
  },

  "minimum_chrome_version": "76.0.0"
}